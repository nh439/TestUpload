@{
}
@if (ViewBag != null && !string.IsNullOrEmpty(ViewBag.result))
{
    @if (ViewBag.result != "Registerd Successful")
    {
       <script>
           Swal.fire({
               icon: 'error',
               title: 'Oops...',
               text: @ViewBag.result,
           })
       </script>
        <meta http-equiv="refresh" content="3; url=/user/register" />
    }
    @if (ViewBag.result == "Registerd Successful")
    {
       <script>
           Swal.fire(
               'Congratulation',
               'Registerd Completed',
               'success'
           )
       </script>
        <meta http-equiv="refresh" content="3; url = /" />

    }
}
